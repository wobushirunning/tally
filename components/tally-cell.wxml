<!--
  index: int,
  money: string
  detail: string
  time: string
-->
<template name="tally-cell">
  <view class="tally-cell">
      <text class="detail-text">{{detail}}</text>
      <text class="money-text">{{money}}元</text>
      <text class="time-text">{{time}}</text>
  </view>
  <!--左滑按钮部分-->
  <view class="tally-set">
      <view class="edit">修改</view>
      <view class="del">删除</view>
  </view>
</template>